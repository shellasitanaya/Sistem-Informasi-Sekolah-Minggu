<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="historiKelasAncPane" prefHeight="574.0" prefWidth="863.0" stylesheets="@../../../../../../../style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pbo_sekolahminggu.controllers.transactional.data.HistoriKelasAnakController">

   <styleClass>
      <String fx:value="white_bg" />
      <String fx:value="shadow" />
   </styleClass>
   <children>
      <TableView fx:id="historiKelasTbl" layoutX="26.0" layoutY="68.0" prefHeight="203.0" prefWidth="812.0" styleClass="filter" stylesheets="@../../../../../../../style.css">
         <columns>
            <TableColumn text="ID Histori" />
            <TableColumn prefWidth="300.0" text="Nama" />
            <TableColumn prefWidth="145.0" text="NIS" />
            <TableColumn prefWidth="150.0" text="Kelas" />
            <TableColumn prefWidth="140.0" text="Tahun Ajaran" />
         </columns>
      </TableView>
      <Pane layoutX="26.0" layoutY="14.0" prefHeight="40.0" prefWidth="256.0" stylesheets="@../../../../../../../style.css">
         <children>
            <TextField fx:id="historiKelasSearchField" layoutX="13.0" layoutY="8.0" onKeyPressed="#Search" prefHeight="25.0" prefWidth="231.0" promptText=" Search Here" styleClass="search_field" stylesheets="@../../../../../../../style.css" />
         </children>
         <styleClass>
            <String fx:value="search_box_outer" />
            <String fx:value="shadow" />
         </styleClass>
      </Pane>
      <Label layoutX="26.0" layoutY="380.0" text="Kelas" />
      <Button fx:id="showHistoriKelas" layoutX="657.0" layoutY="511.0" mnemonicParsing="false" onAction="#showFilter" prefHeight="40.0" prefWidth="80.0" stylesheets="@../../../../../../../style.css" text="SHOW">
         <styleClass>
            <String fx:value="btn_clear" />
            <String fx:value="shadow2" />
         </styleClass>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>

      <Button fx:id="ediHistoriKelas" layoutX="745.0" layoutY="511.0" mnemonicParsing="false" onAction="#editAssign" prefHeight="40.0" prefWidth="80.0" stylesheets="@../../../../../../../style.css" text="EDIT">

         <styleClass>
            <String fx:value="btn_updatedkk" />
            <String fx:value="shadow2" />
         </styleClass>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label layoutX="77.0" layoutY="520.0" text="EXPORT" visible="false">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ImageView fitHeight="43.0" fitWidth="43.0" layoutX="27.0" layoutY="504.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../../images/exportIcon.png" />
         </image>
      </ImageView>
      <Label layoutX="339.0" layoutY="280.0" text="DATA HISTORI KELAS ANAK">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="338.0" text="Tahun Ajaran" />
      <Button fx:id="exportHistoriKelasBtn" layoutX="27.0" layoutY="511.0" mnemonicParsing="false" onMouseClicked="#export" prefHeight="40.0" prefWidth="101.0" style="-fx-opacity: 0.5;" stylesheets="@../../../../../../../style.css" text="EXPORT">
         <graphic>
            <ImageView fitHeight="31.0" fitWidth="27.0" onMouseClicked="#export" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/exportIcon.png" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <styleClass>
            <String fx:value="btn_export" />
            <String fx:value="shadow2" />
         </styleClass>
       </Button>
      <ComboBox fx:id="tahunAjaranHistoriKelasCb" layoutX="132.0" layoutY="338.0" prefWidth="150.0" styleClass="textfield" stylesheets="@../../../../../style.css" />
      <ComboBox fx:id="kelasHistoriKelasCb" layoutX="132.0" layoutY="380.0" prefWidth="150.0" styleClass="textfield" stylesheets="@../../../../../style.css" />
      <Label layoutX="26.0" layoutY="487.0" text="isi semua input field sebelum melakukan export!" textFill="RED">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
